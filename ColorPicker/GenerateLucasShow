    Cmd('Store Preset 4.6')
    Cmd('Label Preset 4.6 "Yellow"')

    Cmd('At Gel "MA"."Fern Green"')
    Cmd('Store Preset 4.7')
    Cmd('Label Preset 4.7 "Fern Green"')

    Cmd('At Gel "MA"."Green"')
    Cmd('Store Preset 4.8')
    Cmd('Label Preset 4.8 "Green"')

    Cmd('At Gel "MA"."Sea Green"')
