    Cmd('Label Preset 4.2 "CTO"')

    Cmd('At Gel "Cinegel"."Half Blue CTB"')
    Cmd('Store Preset 4.3')
    Cmd('Label Preset 4.3 "CTB"')

    Cmd('At Gel "MA"."red"')
    Cmd('Store Preset 4.4')
    Cmd('Label Preset 4.4 "Red"')

